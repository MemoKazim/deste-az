<%-include("partials/header", {title: title, nav: nav})-%>
<div class="asset-1">
  <%-include("partials/goBack", {nav: nav})-%>
  <table class="styled-table table-1">
    <thead>
      <tr>
        <td>Name</td>
        <td>Surname</td>
        <td>Content</td>
        <td>Image</td>
        <td>Date</td>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%-result.name-%></td>
        <td><%-result.surname-%></td>
        <td><%-result.content-%></td>
        <td>
            <img src="/images/<%=result.image%>" alt="Image" width="400rem" height="auto"/>
        </td>
        <td><%-result.date.getDate()-%>-<%-result.date.getMonth()+1-%>-<%-result.date.getFullYear()-%></td>
      </tr>
    </tbody>
  </table>
</div>
<%-include("partials/footer")-%>